{
  "name": "duckdb",
  "main": "./dist/duckdb.js",
  "version": "4.0.0",
  "description": "DuckDB node.js API",
  "gypfile": true,
  "dependencies": {
    "@mapbox/node-pre-gyp": "^1.0.0",
    "node-addon-api": "*",
    "node-gyp": "^7.1.2"
  },
  "binary": {
    "module_name": "duckdb",
    "module_path": "./lib/binding/",
    "host": "https://duckdb-node.s3.amazonaws.com"
  },
  "scripts": {
    "build": "tsc",
    "install": "node-pre-gyp install --fallback-to-build",
    "pretest": "ts-node test/support/createdb.ts",
    "test": "mocha -r ts-node/register -R spec --timeout 480000",
    "pack": "node-pre-gyp package"
  },
  "directories": {
    "lib": "lib",
    "dist": "dist",
    "test": "test"
  },
  "devDependencies": {
    "@types/node": "^18.0.4",
    "aws-sdk": "^2.790.0",
    "mocha": "^8.3.0",
    "ts-node": "^10.9.1",
    "typescript": "^4.7.4"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/cwida/duckdb.git"
  },
  "keywords": [
